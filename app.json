    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.famil.morse",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "9b616056-bb6a-4e8e-be7f-c9e945a871b4"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/9b616056-bb6a-4e8e-be7f-c9e945a871b4"
    },
    "plugins": [
      "expo-asset",
    ]
  }
